<?php
if(isset($_["gpio"])){
	$gpio = $_["gpio"];
	$pinstate_command = "/usr/local/bin/gpio read ".$gpio;
	$data = trim(shell_exec($pinstate_command));
	echo $data;
}
else
{
	if (!isset($gpio)){
		echo "UNSPECIFIED";
	}
	else{
		$pinstate_command = "/usr/local/bin/gpio read ".$gpio;
		$data = trim(shell_exec($pinstate_command));
	}
}

?>