<?php
if(isset($_["name"])){
	$object_name = $_["name"];
}

$object_db = new Entity($object_name);
$objects = $object_db->populate();
if($objects){

foreach($objects as $object){
	$data[] = [
	"text" => $object["object_name"],
	"value" => $object["id"]
	 ];
}
}
else
{
	$data = false;
}


if(isset($_["data_id"])){
 	$data_json["data_id"] = $_["data_id"];
 	$data_json["data"] = $data;
	echo json_encode($data_json);
}
?>