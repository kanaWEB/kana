<?php

$object_name = $menu_selected;
$object_db = new JsonConfig($object_name);


$fields = $_;
unset($fields["submit"]);
unset($fields["menu"]);
unset($fields["tab"]);
unset($fields["category"]);

if (isset($_["id"])) {
    $object_db->setId($_["id"]);
} else {
    $object_db->create();
}

$object_db->save($fields);
var_dump($fields);
redirect("settings", "?category=objects&menu=".$object_name."&tab=".$tab_selected."&notice=".t("Configuration updated"));

